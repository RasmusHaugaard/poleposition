{
  "name": "poleposition",
  "description": "poleposition web interface",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "media/calculator-16.png", "128": "media/calculator-128.png" },
  "permissions": [
		"storage",
    "serial",
		"audioCapture",
		{
			"fileSystem":
			[
				"write",
				"retainEntries",
				"directory"
			]
		}
  ]
}
